{
  "ci": {
    "collect": {
      "staticDistDir": "./_site",
      "settings": {
        "onlyCategories": [
          "performance"
        ]
      }
    },
    "assert": {
      "preset": "lighthouse:no-pwa",
      "assertions": {
        "categories:performance": [
          "error",
          {
            "minScore": 1
          }
        ]
      }
    }
  }
}
