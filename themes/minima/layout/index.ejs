<h2>Posts</h2>
<table>
<tr>
  <th>
    Date
  </th>
  <th>
    Title
  </th>
</tr>
<% site.posts.sort('date', 'desc').forEach(function(post){ %>
<tr>
  <td>
      <%- full_date(post.date, 'll') %>
  </td>
  <td>
    <a href="<%- url_for(post.path) %>" >
      <%- post.title %>
    </a>
  </td>
</tr>
<% }) %>
</table>
